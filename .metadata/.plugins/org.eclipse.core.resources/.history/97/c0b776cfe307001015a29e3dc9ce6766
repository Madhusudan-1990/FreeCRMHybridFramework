package com.qa.crm.pages;

import org.openqa.selenium.By;

public class HomePage 
{
	By homeIcon = By.xpath("//i[@class = 'home icon']");
	By headers = By.cssSelector("div[class='header']");
	By search = By.xpath("//input[@placeholder='Search']");
	By searchIcon = By.xpath("//i[@class='black search icon']");
	By calendarIcon = By.xpath("//i[@class = 'calendar icon']");
	By contactsIcon = By.xpath("//i[@class = 'users icon']");
}
